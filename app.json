{
  "expo": {
    "name": "GTM - Demandeur",
    "slug": "gtm-demandeur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-gtm.png",
    "scheme": "gtmdemandeur",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-gtm.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.achaumba.gtmdemandeur"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo-gtm.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo-gtm.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c7d1fc1c-b5df-4105-9c8b-e2ffd32a87c2"
      }
    }
  }
}
